<?xml version="1.0"?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../../../doc/drakeURDF.xsd" name="Brick">

  <link name="brick">
    <inertial>
      <mass value="50"/>
      <inertia ixx="50" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
    </inertial>
    <visual>
      <geometry>
        <box size="2 .5 1" />
      </geometry>
    </visual>
    <collision group="brick">
      <geometry>
        <box size="2 .5 1" />
      </geometry>
    </collision>
  </link>
</robot>

